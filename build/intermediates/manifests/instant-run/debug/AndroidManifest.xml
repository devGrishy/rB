<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grishon.rb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />

        <activity android:name="com.grishon.rb.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.grishon.rb.Verify" />
        <activity android:name="com.grishon.rb.Welcome" />
        <activity android:name="com.grishon.rb.CreateNewCompany" />
        <activity android:name="com.grishon.rb.NewCompany" />
        <activity android:name="com.grishon.rb.Clients" />
        <activity android:name="com.grishon.rb.Analyze" />
        <activity android:name="com.grishon.rb.Record" />
        <activity android:name="com.grishon.rb.Payments" />
        <activity android:name="com.grishon.rb.Settings" />
        <activity android:name="com.grishon.rb.BaseActivity" />
        <activity android:name="com.grishon.rb.RegClient" />
        <activity android:name="com.grishon.rb.Analyze$SpinnerActivity" />
        <activity android:name="com.grishon.rb.Record$SpinnerActivity" />

        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.grishon.rb.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>